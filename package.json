{
  "name": "Metromed-UC",
  "version": "2.5.2",
  "description": "Healthcare webapp with Webpack, React and Material-UI",
  "repository": {
    "type": "git",
    "url": "https://github.com/aidarbuy/metromedex.git"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "webpack-dev-server --config webpack-dev.js --progress --inline --https --history-api-fallback",
    "start:prod": "webpack --config webpack-prod.js --progress",
    "stats": "webpack --config webpack-production.config.js --progress --colors --json --profile >stats.json"
  },
  "private": true,
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.24.0",
    "html-webpack-plugin": "^2.7.2",
    "if-env": "^1.0.0",
    "image-webpack-loader": "^2.0.0",
    "node-sass": "^3.8.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "transfer-webpack-plugin": "^0.1.4",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1"
  },
  "dependencies": {
    "firebase": "^3.3.0",
    "google-maps": "^3.2.1",
    "lodash": "^4.15.0",
    "material-ui": "^0.15.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "socket.io-client": "^1.4.8",
    "webrtc-adapter": "^2.0.2"
  }
}
